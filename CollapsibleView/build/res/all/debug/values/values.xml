<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CollapsibleLayout">
        <attr name="hiddenStateDrawable" format="reference" />
        <attr name="visibleStateDrawable" format="reference" />
        <attr name="collapsibleLayoutActionId" format="reference" />
        <attr name="collapsibleLayoutStateIndicatorId" format="reference" />
        <attr name="collapsibleLayoutContentId" format="reference" />
    </declare-styleable>

    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>

    <string name="action_settings">Settings</string>
    <string name="app_name">CollapsibleView</string>
    <string name="collapsibleLayoutAction">collapsibleLayoutActionView</string>
    <string name="collapsibleLayoutContent">collapsibleLayoutContent</string>
    <string name="collapsibleLayoutStateIndicator">collapsibleLayoutStateIndicator</string>
    <string name="hello_world">Hello world!</string>

    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>
